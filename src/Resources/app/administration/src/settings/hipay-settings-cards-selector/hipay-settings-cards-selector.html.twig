<sw-card position-identifier="sw-settings-payment-detail-content"
  :title="$tc('hipay.settings.creditcard.title')"
  :is-loading="isLoading">
  <sw-multi-select :label="$tc('hipay.settings.creditcard.choice')"
    :options="availableCards"
    :value="paymentMethod.customFields?.cards || []"
    @update:value="updateCardsValue"></sw-multi-select>
</sw-card>
